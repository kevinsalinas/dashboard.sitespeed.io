{
  "extends": "./config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "external",
      "profile": "cable"
    },
    "visualElements": true,
    "iterations": 7,
    "browser": "chrome",
    "video": true,
    "visualMetric": true
  },
  "chrome": {
    "collectConsoleLog": true,
    "includeResponseBodies": "html"
  },
  "firefox": {
    "includeResponseBodies": "html",
    "disableBrowsertimeExtension": true
  },
  "pageLoadStrategy": "none",
  "pageCompleteCheckStartWait": 5000,
  "pageCompleteCheckPollTimeout": 1000,
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  }
}
